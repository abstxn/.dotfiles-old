{
    "title": "Move between spaces.",
    "rules": [
        {
            "description": "Binds hyper + [U|I] to move to [left|right] spaces.",
            "manipulators": [
                {
                    "from": {
                        "key_code": "u",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_option",
                                "left_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_arrow",
                            "modifiers": [
                            "left_control"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "i",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_option",
                                "left_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_arrow",
                            "modifiers": [
                            "left_control"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}